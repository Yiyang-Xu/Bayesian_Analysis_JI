   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "from scipy.stats import uniform\n",
    "\n",
    "N = 10000\n",
    "u = uniform(0,1).rvs(N)\n",
    "lamb = 10\n",
    "x = -np.log(1-u) / lamb\n",
    "\n",
    "plt.hist(x, bins = 50, density = True)\n",
    "plt.title('Simulation of Exponential Distribution')"
   ]
  },
